---
layout: "_document.webc"
pagination:
  data: collections
  size: 1
  alias: tag
  filter: 
    - all
    - templatesByTags
    - post
permalink: "`/tag/${tag}/`"
---

<h1 @text="tag"></h1>
<ul>
  <li webc:for="entry of collections[tag]">
    <a :href="entry.url" @text="entry.data.title"></a>
  </li>
</ul>
